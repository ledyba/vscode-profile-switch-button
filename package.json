{
  "name": "profile-switch-button",
  "displayName": "Profile Switch Button",
  "description": "Switch your profile!",
  "icon": "icon.png",
  "publisher": "ledyba",
  "author": "Kaede Fujisaki",
  "license": "MIT",
  "homepage": "https://github.com/ledyba/vscode-profile-switch-button",
  "repository": {
    "type": "git",
    "url": "https://github.com/ledyba/vscode-profile-switch-button.git"
  },
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test",
    "up": "npm-check-updates -u && npm install && npm dedupe && npm audit fix"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "^22.7.5",
    "@types/vscode": "^1.94.0",
    "@typescript-eslint/eslint-plugin": "^8.9.0",
    "@typescript-eslint/parser": "^8.9.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.24.0",
    "eslint": "^9.12.0",
    "npm-check-updates": "^17.1.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.6.3"
  }
}
